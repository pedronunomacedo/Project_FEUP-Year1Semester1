$(function () {
    var $forms = $('.formRequestSomethingToEmail');
    var $form = $forms[0];
    var messages = window.requestSomethingToEmailMessages;
    $form.Email.startPlaceholder = $form.Email.placeholder;

    var $loaderPanel = $('.loaderSomethingToEmail');
    var $errorPanel = $('.errorSomethingToEmail');
    var $successPanel = $('.successSomethingToEmail');
    var $formPanel = $('.formRequestSomethingToEmailMessageUnderForm');
    var emailRegex = new RegExp(window.emailReg);


    var formsCheckbox = '.formRequestSomethingToEmail ',
        formsCheckboxNew = '.formRequestSomethingToEmail_new ',
        agreeReceiveCheckboxEmail = 'input[name=AgreeReceiveCheckboxEmail]',
        formPanelMessage = ' .formRequestSomethingToEmailMessageUnderForm ',
        formPanelMessageNew = ' formRequestSomethingToEmailMessageUnderForm_new ';
        formPanelMessageClassNew = ' .formRequestSomethingToEmailMessageUnderForm_new '

    window.buttonClock = function () {

        $($form).submit();

        if ($('html').hasClass('touch')) {            

            var footerHtml = $('footer').html();
            var $emptyFooter = $('.emptyFooter');
            var emptyFooterString = ' .emptyFooter ';
            $emptyFooter.html(footerHtml);            

            var $formsClass = $forms.attr('class');            
            $(emptyFooterString + '.' + $forms.attr('class'))
                .removeClass($forms.attr('class'))
                .addClass($formsClass + '_new');

            var $loaderPanelClass = $loaderPanel.attr('class');
            $(emptyFooterString + '.' + $forms.attr('class'))
                .removeClass($loaderPanel.attr('class'))
                .addClass($loaderPanelClass + '_new');            

            var $errorPanelClass = $errorPanel.attr('class');
            $(emptyFooterString + '.' + $errorPanel.attr('class'))
                .removeClass($errorPanel.attr('class'))
                .addClass($errorPanelClass + '_new');            

            var $successPanelClass = $successPanel.attr('class');
            $(emptyFooterString + '.' + $successPanel.attr('class'))
                .removeClass($successPanel.attr('class'))
                .addClass($successPanelClass + '_new');            


            
            var $formPanelClass = $formPanel.attr('class');            
            $(emptyFooterString + formPanelMessage)
                .removeClass("formRequestSomethingToEmailMessageUnderForm")
                .addClass(formPanelMessageNew);            
                        
            if ($(formsCheckbox + agreeReceiveCheckboxEmail).prop('checked')) {
                $(formsCheckboxNew + agreeReceiveCheckboxEmail).prop('checked', true);
            }

            $('.emptyFooter' + ' ' + formsCheckboxNew + 'input[name=Email]').val($('footer' + ' ' + formsCheckbox + 'input[name=Email]').val());
              
            var timerId = setInterval(hasMessageBlogPopup, 2000);

            setTimeout(function () {
                clearInterval(timerId);
            }, 4000);

        }

        
    };

    function hasMessageBlogPopup() {
        if ($('footer .formRequestSomethingToEmailMessageUnderForm').hasClass('hasMessageBlog')) {             
            var $hasMessageBlog = $('footer .formRequestSomethingToEmailMessageUnderForm').html();
            $('.emptyFooter .formRequestSomethingToEmailMessageUnderForm_new').html($hasMessageBlog);            
        }
    } 

    if ($('html').hasClass('touch')) {
        

        $('.emptyFooter').on('keyup', 'input[name=Email]', function (event) {
            var value = this.value.trim();
            $(formsCheckbox + 'input[name=Email]').val(value);

        });

        $('footer').on('keyup', 'input[name=Email]', function (event) {
            var value = this.value.trim();
            $(formsCheckboxNew + 'input[name=Email]').val(value);

        }); 

        //checkbox
        $(".emptyFooter").bind("change click", agreeReceiveCheckboxEmail, function () {

            if ($(formsCheckboxNew + agreeReceiveCheckboxEmail).prop('checked')) {
                $(formsCheckbox + agreeReceiveCheckboxEmail).prop('checked', true).removeClass('valErrorShadow');;
                $(formsCheckboxNew + agreeReceiveCheckboxEmail).removeClass('valErrorShadow');
            } else {
                $(formsCheckbox + agreeReceiveCheckboxEmail).prop('checked', false);
            }

        });

        $("footer").on("click", agreeReceiveCheckboxEmail, function () {

            if ($(formsCheckbox + agreeReceiveCheckboxEmail).prop('checked')) {
                $(formsCheckboxNew + agreeReceiveCheckboxEmail).prop('checked', true).removeClass('valErrorShadow');                
            } else {
                $(formsCheckboxNew + agreeReceiveCheckboxEmail).prop('checked', false);
            }


        });

        //Close checkbox

    }

    //------------------------------

    function defaultState() {
        $formPanel.removeClass('hasMessageBlog');
        $loaderPanel.hide();
        $errorPanel.hide();
        $successPanel.hide();

        if ($('html').hasClass('touch')) {
            $(formPanelMessageClassNew).removeClass('hasMessageBlog');
            $(formsCheckboxNew + 'input[name=Email]').val($(formsCheckbox + 'input[name=Email]').val());
            $(formsCheckboxNew + agreeReceiveCheckboxEmail).prop('checked', false);
        }
    }
    $('footer .formRequestSomethingToEmailMessageUnderForm .gradientBtn').on('click', defaultState);



    if ($('html').hasClass('touch')) {
        $('.emptyFooter').on('click', '.gradientBtn', function () {
            $('footer .formRequestSomethingToEmailMessageUnderForm .gradientBtn').click();
        });
    }

    

    $($form.Email).on('focus', function () {
        setDefaultField($form.Email, true);
        $($form.AgreeReceiveCheckboxEmail).removeClass("valErrorShadow");
    });
    $($form.Email).on('blur', function () {
        setDefaultField($form.Email);
        $($form.AgreeReceiveCheckboxEmail).removeClass("valErrorShadow");
    });

    function waiting(show) {
        $forms.parent().addClass('shadowSectionFooter');
        if (show) {
            $loaderPanel.show();
            $formPanel.addClass('hasMessageBlog');
        } else {
            $loaderPanel.hide();
            $formPanel.removeClass('hasMessageBlog');
        }
    }

    function setDefaultField(el, clearPlaceholder) {
        if ($(el).hasClass('valError'))
            $(el).removeClass('valError');
        if (clearPlaceholder)
            el.placeholder = '';
        else
            el.placeholder = el.startPlaceholder;
        $form.AgreeReceiveEmail.value = '';
    }

    function setInvalidField(el, msg) {
        $(el).addClass("valError");
        el.value = '';
        el.placeholder = msg;
    }

    function errorResponse(error) {
        defaultState();
        console.error(error);
        $formPanel.addClass('hasMessageBlog');
        $errorPanel.show();
    }

    function showResponse(data, context) {
        var result = JSON.parse(data.replace("\r\n"));
        waiting(false);
        $forms.parent().addClass('shadowSectionFooter');
        if (!result.IsSucess) {
            errorResponse(result.Message);
        }
        else {
            $formPanel.addClass('hasMessageBlog');
            $successPanel.show();
        }
    }

    function validateData(formData, jqForm, options) {
        var isValid = true;

        $form.Email.value = $form.Email.value.trim();
        if ($form.Email.value === '') {
            setInvalidField($form.Email, messages.emailMandatory);
            isValid = false;
        } else if (!emailRegex.test($form.Email.value)) {
            setInvalidField($form.Email, messages.emailError);
            isValid = false;
        }
        if ($form.AgreeReceiveCheckboxEmail.checked) {
            var data = $.grep(formData, function (e) { return e.name === 'AgreeReceiveEmail'; });
            if (data.length === 0) {
                formData.push({
                    name: 'AgreeReceiveEmail',
                    value: $form.AgreeReceiveCheckboxEmail.checked
                });
            } else {
                data[0].value = $form.AgreeReceiveCheckboxEmail.checked;
            }
            $($form.AgreeReceiveCheckboxEmail).removeClass("valErrorShadow");
        } else {
            $($form.AgreeReceiveCheckboxEmail).addClass("valErrorShadow");
            isValid = false;
        }
        if (isValid)
            waiting(true);
        return isValid;
    }

    $($form.AgreeReceiveCheckboxEmail).on('click',
        function () {
            $($form.AgreeReceiveCheckboxEmail).removeClass('valErrorShadow');
        });

    var options = {
        //target: '#sendRequestConsultation',   // target element(s) to be updated with server response 
        beforeSubmit: validateData,  // pre-submit callback 
        success: showResponse,  // post-submit callback
        error: errorResponse,
        clearForm: true,  // clear all form fields after successful submit 
        resetForm: true,        // reset the form after successful submit 
        data: { sid: window.sid }
    };

    $forms.ajaxForm(options);

});;
/* PrismJS 1.15.0
