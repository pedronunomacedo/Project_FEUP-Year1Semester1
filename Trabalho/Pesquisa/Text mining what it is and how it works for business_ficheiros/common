//add loader for link transitions in topics 
$(document).ready(function () {
    var $pageloader = $('#LoadMorePreloader');
    $('body').on('click', '.transitionWithLoader', function (event) {
        $pageloader.show();
        //window.location.href = event.currentTarget.attributes.href.value;
    });
});

//add animation to footer with fixed position
function footerAnimation() {
    var heightFooter = $('footer').height(),
        windowHeight = $(window).height(),
        scrollFooter = windowHeight - heightFooter;
    $('.emptyFooter').height(heightFooter).css('max-height', '2500px');

    if ($('html').hasClass('touch')) {
        var footerHtml = $('footer').html();       

        $('.emptyFooter').html(footerHtml);

        $('.emptyFooter .formRequestSomethingToEmail')
            .removeClass("formRequestSomethingToEmail")
            .addClass('formRequestSomethingToEmail_new');

        $('.emptyFooter .loaderSomethingToEmail')
            .removeClass("loaderSomethingToEmail")
            .addClass('loaderSomethingToEmail_new');

        $('.emptyFooter .errorSomethingToEmail')
            .removeClass("errorSomethingToEmail")
            .addClass('errorSomethingToEmail_new');

        $('.emptyFooter .successSomethingToEmail')
            .removeClass("successSomethingToEmail")
            .addClass('successSomethingToEmail_new');

        $('.emptyFooter .formRequestSomethingToEmailMessageUnderForm')
            .removeClass("formRequestSomethingToEmailMessageUnderForm")
            .addClass('formRequestSomethingToEmailMessageUnderForm_new');

    }
    else {
        setTimeout(function () {
            $('.emptyFooter').css('max-height', '1px');
        }, 1500)

        $('footer').attr('data-end', 'top:' + scrollFooter + 'px');
    }
}

//img convert to Svg
function imgSvg(selector = 'img.svg') {
    $(selector).each(function () {
        var $img = jQuery(this);
        var imgID = $img.attr('id');
        var imgClass = $img.attr('class');
        var imgURL = $img.attr('src');
        jQuery.get(imgURL, function (data) {
            var $svg = jQuery(data).find('svg');
            if (typeof imgID !== 'undefined') {
                $svg = $svg.attr('id', imgID);
            }
            if (typeof imgClass !== 'undefined') {
                $svg = $svg.attr('class', imgClass + ' replaced-svg');
            }
            $svg = $svg.removeAttr('xmlns:a');
            $img.replaceWith($svg);
        }, 'xml');

    });
}

//animation case
function unactiveImgShowAnim() {
    $('.skrollable-after.unactiveImgShowAnim').each(function () {
        $(this).removeClass('unactiveImgShowAnim');
    })
}

//simpleTriggerImg
function simpleTriggerImg() {
    $('.carouselImgCustom').on('click', 'img.unactive', function () {
        $(this).siblings().addClass('unactive');
        $(this).removeClass('unactive');
    })
}

//yearVisibilitySwitcher
function yearVisibilitySwitcher() {
    if ($(window).width() > 1800) {
        $('.caseYearHeader').hide();
        $('.caseYearSelector').show();
    } else {
        $('.caseYearSelector').hide();
        $('.caseYearHeader').show();
    }
};

//calling functions
$(window).on("load resize", function () {
    yearVisibilitySwitcher();
    footerAnimation();
    skrollr.init().refresh();
})

$(window).on("scroll resize touchmove", function () {
    unactiveImgShowAnim();
})

simpleTriggerImg();
setTimeout(unactiveImgShowAnim, 700);



//hide and show when popup is opened or closed
function scrollbarWidth() {
    var div = $('<div style="width:50px;height:50px;overflow:hidden;position:absolute;top:-200px;left:-200px;"><div style="height:100px;"></div>');
    // Append our div, do our calculation and then remove it 
    $('body').append(div);
    var w1 = $('div', div).innerWidth();
    div.css('overflow-y', 'scroll');
    var w2 = $('div', div).innerWidth();
    $(div).remove();
    var scrollWidth = w1 - w2;
    return (scrollWidth);
}
$(function () {
    window.widthScroll = scrollbarWidth();
});

var ua = navigator.userAgent,
       scrollTopPosition,
       iOS = /iPad|iPhone|iPod/.test(ua),
       iOS11 = /OS [1-9][1-9]/.test(ua);
       iOS11_3Next = /OS 11_[3-9]+|OS 11_[0-9]+[0-9]+|OS [1-9][2-9]/.test(ua);

if (iOS11_3Next) {
    $('html').addClass('iOS11_3Next');
}

function addPaddPopup() {
    $('html').addClass('showMenu');
    $('body').css({
        'overflow': 'hidden',
        'padding-right': widthScroll
    });
    $('.navbar, footer').css('padding-right', widthScroll)
    $('.popupWrapFixed').on('touchmove touchend touchstart', function () {
        event.stopPropagation();
    })
}
function removePaddPopup() {
    setTimeout(function () {
        $('#searchPostValue').blur();
        $('html').removeClass('showMenu');
        $('body').css('padding-right', '');
        if ($('html').hasClass('no-touch')) {
            $('body').css('overflow', '');
        }
        $('.navbar, footer').css('padding-right', '')
    }, 300)
}
//END hide and show when popup is opened or closed


$(function () {

    $('.helperIos').on('scroll', function () {
        $(this).scrollTop(20);
    })

    //linkToTop
    $('.footerBottomLink').click(function () {
        skrollr.init().animateTo(0);
    });

    //scroll from hero to content
    $('.scrollIco').click(function () {
        var scrollTooContent = $('#topSection').height();
        skrollr.init().animateTo(scrollTooContent);
    });

   //localStorage anchore
    $('.anchorCategory').on('click', function () {
        var val = $(this).data('anchor');
        window.localStorage.setItem('categoryRout', val);
    });
    var activeBtn = window.localStorage.getItem('categoryRout');
    if (activeBtn) {
        $('[data-anchor="' + activeBtn + '"]').addClass("activeAnchor").removeAttr("href").css("cursor", "default");
    }
    window.localStorage.setItem('categoryRout', '');

    //hover touch
    var blogItems = $('.touch .touchHover');
    blogItems.removeClass('hover');
    blogItems.on('click', function (e) {
        blogItems.removeClass('hover')
        $(this).addClass('hover');
        var elemAnchorBlog = $(this).find('a');
        setTimeout(function () {
            if ($(e.target).hasClass('anchorCategory')) {
                window.location = $(e.target).attr("href");
            }
            else {
                window.location = elemAnchorBlog[0].href;
            }
        }, 100);
        return false;
    });

   


    //toggleMenu
    function toggleMenu(selector, container) {
        $('.' + selector).click(function () {
            $('.' +container).addClass('in');
            $('html').addClass('showMenu disabledAnim');
            addPaddPopup();
            $('.' + container + '.in').on('touchmove touchend touchstart', function () {
                event.stopPropagation();
            });
            $('#searchPostValue').focus();
            if (/iPhone|iPad|iPod/i.test(navigator.userAgent)) {
                $('#searchPostValue').blur();
            }
        });
        $('.close-navbar').click(function () {
            $('.' + container).removeClass('in');
            removePaddPopup();
            $('html').removeClass('showMenu');
            setTimeout(function () {
                $('html').removeClass('disabledAnim');
            }, 500)
        })
    }

    

    

    //called function
    var timeout = 500;
    if (/iPhone|iPad|iPod/i.test(navigator.userAgent)) {
        timeout = 1000;        
    }
    setTimeout(function () {
        var s = skrollr.init({
            mobileDeceleration: 0.01,
            render: function (data) {
                $('body').css('opacity', '1');
            }
        });
        $('body').css('opacity', '1');        
        skrollr.menu.init(s, {
            duration: function (currentTop, targetTop) {                
                return Math.abs(currentTop - targetTop) * 0.15;
            },
            easing: 'linear'
        });
    }, timeout);

    //detect mac
    if (navigator.userAgent.indexOf('Mac') > 0) {
        $('body').addClass('mac-os');
    }

    //fix inputs subscribe form focus
    $(window).on('resize', function () {
        var eVal = $('.sendMassageBlog input[name="Email"]').val();
        var eCheck = $('.sendMassageBlog .checkboxPadd input[type="checkbox"]');
        if ($('.sendMassageBlog input[name="Email"]').is(":focus")) {
            $('.sendMassageBlog input[name="Email"]').val(eVal);
            skrollr.init().refresh();
        } else if ($('.sendMassageBlog .checkboxPadd input[type="checkbox"]').is(":checked")) {
            eCheck.prop('checked');
        } else {
            $('body').addClass('disabledAnim');
            setTimeout(function () {
                footerAnimation();
                skrollr.init().refresh();
                $('body').removeClass('disabledAnim');
                $('.extraWrap').attr('style', 'height: 100%;');
                $('.sendMassageBlog input[name="Email"]').val(eVal);
                $('.sendMassageBlog .checkboxPadd input[type="checkbox"]').val(eCheck);
            }, 100);
        }
    });

    //footerAnimation();
    //skrollr.init().refresh();
    imgSvg();
    simpleTriggerImg();
    toggleMenu('menu-toggle', 'navbar-collapse');
    toggleMenu('linkTopics', 'topicsTagMenuWrap');
    toggleMenu('menu-toggle-search', 'searchMenuWrap');

    $('a[href$=".pdf"]').each(function (index, element) {
        element.href += "#zoom=page-height";
    });


});;
